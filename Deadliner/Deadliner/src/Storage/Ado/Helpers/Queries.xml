<?xml version="1.0" encoding="utf-8"?>
<Root>
    <Table Name="CUSTOM_COMMANDS">
    </Table>
    <Table Name="Users">
        <Command Name="GetAllUsers">
            select Id, Username, Password from [dbo].[Users]
        </Command>
        <Command Name="GetUserById">
            select Id, Username, Password from [dbo].[Users] where ProductId=@productId
        </Command>
    </Table>
</Root>
