<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:DeadlinerUI.View"
                    xmlns:viewModel="clr-namespace:DeadlinerUI.ViewModel">

    <DataTemplate DataType="{x:Type viewModel:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:CalendarVm}">
        <view:Calendar />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:EventsVm}">
        <view:Events />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:TasksVm}">
        <view:Tasks />
    </DataTemplate>

</ResourceDictionary>